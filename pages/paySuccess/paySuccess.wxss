/* paySucess.wxss */
.sucessView{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* background-color: orange; */
  width: 100%;
  height: 400rpx;
  
}
.successText{
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #333;
}
.tabBarView
{
  display: flex;
  justify-content: space-around;
  width: 690rpx;
  height: 98rpx;
  background-color: white;
  position: fixed;
  bottom:0;
  left: 30rpx;
  right: 30rpx;  
}

.backToIndex{
  font-size: 36rpx;
  color: white;
  background-color: #52a3fa;
  border-radius: 10rpx;
  height: 96rpx;
  width:100%;
}
.orderView{
  background-color: #fafafa;
  width: 690rpx;
  height: 228rpx;
  border-radius: 10rpx;
  margin-left: 30rpx;
}
.topView{
  display: flex;
  width: 100%;
  height: 130rpx;
}
.orderImage{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  margin-left: 30rpx;
  margin-top: 30rpx;
}
.descView{
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 300rpx;
  /* background-color: orange; */
  height: 100rpx;
  margin-left: 20rpx;
  margin-top: 30rpx;
}
.totalMoneyText{
   font-size: 28rpx;
   color: #666;
   margin-bottom: 12rpx;
}
.timeText{
  font-size: 24rpx;
  color: #999;
  margin-top: 12rpx; 
}
.bottomView{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  /* background-color: orange; */
  width: 100%;
  height: 80rpx;
}
.arrowImage{
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
}
.detailText{
  font-size: 24rpx;
  color: #999;
}