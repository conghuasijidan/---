/* pay.wxss */
.separateView{
  background-color: #fafafa;
  height: 20rpx;
  width: 100%;
}
.cellView{
  display: flex;
  align-items: center;
  width: 100%;
  height: 120rpx;
   /* background-color: orange;  */
}
.totalMoneyText{
  font-size: 28rpx;
  color: #666;
  margin-left: 30rpx;
}
.moneyText{
  font-size: 48rpx;
  color: #fd3c5a;
  margin-left: 20rpx;
}
.payView{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 120rpx;
   /* background-color: orange;  */
}
.leftView{
  display: flex;
  align-items: center;
  height: 50rpx;
  width: 200rpx;
  /* background-color: beige; */
  margin-left: 30rpx;
}
.payIconImage{
  width: 50rpx;
  height: 50rpx;
}
.payText{
  font-size: 28rpx;
  color: #333;
  margin-left: 20rpx;
}
.checkImage{
  width: 50rpx;
  height: 50rpx;
  margin-right: 30rpx;
}
.uncheckView{
  width: 45rpx;
  height: 45rpx;
  border-radius: 30rpx;
  border:2px solid #999;
  margin-right: 30rpx;
}
.lineView{
  background-color: #fafafa;
  height: 2rpx;
  width: 100%;
}
.notiView{
  display: flex;
  width: 100%;
  height: 120rpx;
}
.notiText{
  margin-top: 30rpx;
  margin-left: 30rpx;
  font-size: 20rpx;
  color: #999;
}
.bottomView{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #52a3fa;
  width: 690rpx;
  height: 98rpx;
  border-radius: 10rpx;
  position: fixed;
  left: 30rpx;
  right: 30rpx;
  bottom: 0rpx;

}
.bottomText{
  color: white;
  font-size: 36rpx;
}